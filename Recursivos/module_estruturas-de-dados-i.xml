<?xml version="1.0" encoding="UTF-8"?>
<project name="module_estruturas-de-dados-i" default="compile.module.estruturas-de-dados-i">
  <dirname property="module.estruturas-de-dados-i.basedir" file="${ant.file.module_estruturas-de-dados-i}"/>
  
  <property name="module.jdk.home.estruturas-de-dados-i" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.estruturas-de-dados-i" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.estruturas-de-dados-i" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.estruturas-de-dados-i" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="estruturas-de-dados-i.output.dir" value="${module.estruturas-de-dados-i.basedir}/out/production/estruturas-de-dados-I"/>
  <property name="estruturas-de-dados-i.testoutput.dir" value="${module.estruturas-de-dados-i.basedir}/out/test/estruturas-de-dados-I"/>
  
  <path id="estruturas-de-dados-i.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="estruturas-de-dados-i.module.production.classpath">
    <path refid="${module.jdk.classpath.estruturas-de-dados-i}"/>
  </path>
  
  <path id="estruturas-de-dados-i.runtime.production.module.classpath">
    <pathelement location="${estruturas-de-dados-i.output.dir}"/>
  </path>
  
  <path id="estruturas-de-dados-i.module.classpath">
    <path refid="${module.jdk.classpath.estruturas-de-dados-i}"/>
    <pathelement location="${estruturas-de-dados-i.output.dir}"/>
  </path>
  
  <path id="estruturas-de-dados-i.runtime.module.classpath">
    <pathelement location="${estruturas-de-dados-i.testoutput.dir}"/>
    <pathelement location="${estruturas-de-dados-i.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.estruturas-de-dados-i">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.estruturas-de-dados-i">
    <patternset refid="excluded.from.module.estruturas-de-dados-i"/>
  </patternset>
  
  <path id="estruturas-de-dados-i.module.sourcepath">
    <dirset dir="${module.estruturas-de-dados-i.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.estruturas-de-dados-i" depends="compile.module.estruturas-de-dados-i.production,compile.module.estruturas-de-dados-i.tests" description="Compile module estruturas-de-dados-I"/>
  
  <target name="compile.module.estruturas-de-dados-i.production" depends="register.custom.compilers" description="Compile module estruturas-de-dados-I; production classes">
    <mkdir dir="${estruturas-de-dados-i.output.dir}"/>
    <javac2 destdir="${estruturas-de-dados-i.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.estruturas-de-dados-i}/javac">
      <compilerarg line="${compiler.args.estruturas-de-dados-i}"/>
      <bootclasspath refid="estruturas-de-dados-i.module.bootclasspath"/>
      <classpath refid="estruturas-de-dados-i.module.production.classpath"/>
      <src refid="estruturas-de-dados-i.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.estruturas-de-dados-i"/>
    </javac2>
    
    <copy todir="${estruturas-de-dados-i.output.dir}">
      <fileset dir="${module.estruturas-de-dados-i.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.estruturas-de-dados-i.tests" depends="register.custom.compilers,compile.module.estruturas-de-dados-i.production" description="compile module estruturas-de-dados-I; test classes" unless="skip.tests"/>
  
  <target name="clean.module.estruturas-de-dados-i" description="cleanup module">
    <delete dir="${estruturas-de-dados-i.output.dir}"/>
    <delete dir="${estruturas-de-dados-i.testoutput.dir}"/>
  </target>
</project>